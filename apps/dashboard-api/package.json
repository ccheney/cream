{
	"name": "@cream/dashboard-api",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"main": "./src/index.ts",
	"types": "./src/index.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		}
	},
	"scripts": {
		"dev": "PORT=3001 bun run --env-file=../../.env --watch src/index.ts",
		"build": "bun build src/index.ts --outdir dist --target bun",
		"start": "bun run dist/index.js",
		"typecheck": "tsgo --noEmit",
		"test": "NODE_ENV=test bun test",
		"lint": "biome check src/",
		"format": "biome format --write src/"
	},
	"dependencies": {
		"@ai-sdk/google": "3.0.9",
		"@cream/agents": "workspace:*",
		"@cream/mastra": "workspace:*",
		"@cream/broker": "workspace:*",
		"@cream/config": "workspace:*",
		"@cream/dashboard-types": "workspace:*",
		"@cream/domain": "workspace:*",
		"@cream/filings": "workspace:*",
		"@cream/helix": "workspace:*",
		"@cream/indicators": "workspace:*",
		"@cream/logger": "workspace:*",
		"@cream/marketdata": "workspace:*",
		"@cream/schema-gen": "workspace:*",
		"@connectrpc/connect": "2.1.1",
		"@connectrpc/connect-node": "2.1.1",
		"@cream/metrics": "workspace:*",
		"@cream/storage": "workspace:*",
		"@cream/universe": "workspace:*",
		"@hono/zod-openapi": "1.2.0",
		"@opentelemetry/api": "1.9.0",
		"@opentelemetry/auto-instrumentations-node": "0.68.0",
		"@opentelemetry/core": "2.4.0",
		"@opentelemetry/exporter-trace-otlp-proto": "0.210.0",
		"@opentelemetry/sdk-node": "0.210.0",
		"@opentelemetry/sdk-trace-base": "2.4.0",
		"ai": "6.0.37",
		"better-auth": "1.4.13",
		"hono": "4.11.4",
		"zod": "4.3.5"
	},
	"devDependencies": {
		"@cream/tsconfig": "workspace:*",
		"@types/bun": "1.3.6",
		"@typescript/native-preview": "7.0.0-dev.20260115.1"
	}
}
