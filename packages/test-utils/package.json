{
	"name": "@cream/test-utils",
	"version": "0.1.0",
	"description": "Shared test helpers for Cream packages",
	"type": "module",
	"main": "src/index.ts",
	"exports": {
		".": "./src/index.ts"
	},
	"scripts": {
		"build": "bun build src/index.ts --outdir dist --target bun",
		"test": "bun test",
		"typecheck": "tsgo --noEmit",
		"lint": "biome check src/ tests/",
		"format": "biome format --write src/ tests/"
	},
	"devDependencies": {
		"@cream/tsconfig": "workspace:*",
		"@types/bun": "1.3.6",
		"@typescript/native-preview": "7.0.0-dev.20260122.4"
	}
}
