{
	"name": "@cream/domain",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./src/index.ts",
			"import": "./src/index.ts"
		},
		"./env": {
			"types": "./src/env.ts",
			"import": "./src/env.ts"
		},
		"./schemas": {
			"types": "./src/schemas/index.ts",
			"import": "./src/schemas/index.ts"
		},
		"./time": {
			"types": "./src/time.ts",
			"import": "./src/time.ts"
		},
		"./websocket": {
			"types": "./src/websocket/index.ts",
			"import": "./src/websocket/index.ts"
		},
		"./grpc": {
			"types": "./src/grpc/index.ts",
			"import": "./src/grpc/index.ts"
		},
		"./calendar": {
			"types": "./src/calendar/index.ts",
			"import": "./src/calendar/index.ts"
		}
	},
	"scripts": {
		"build": "bun build ./src/index.ts --outdir ./dist --target bun",
		"typecheck": "tsgo --noEmit",
		"test": "bun test",
		"lint": "biome check src/",
		"format": "biome format --write src/",
		"clean": "rm -rf dist coverage"
	},
	"dependencies": {
		"@bufbuild/protobuf": "2.11.0",
		"@connectrpc/connect": "2.1.1",
		"@connectrpc/connect-node": "2.1.1",
		"@cream/logger": "workspace:*",
		"@cream/schema-gen": "workspace:*",
		"zod": "4.3.6"
	},
	"devDependencies": {
		"@cream/tsconfig": "workspace:*",
		"@types/bun": "1.3.8",
		"@typescript/native-preview": "7.0.0-dev.20260203.1"
	}
}
