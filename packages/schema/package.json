{
  "name": "@cream/schema",
  "version": "0.0.1",
  "private": true,
  "description": "Protobuf schema definitions for Cream trading system",
  "scripts": {
    "lint": "buf lint",
    "breaking": "buf breaking --against '../../.git#branch=master,subdir=packages/schema'",
    "generate": "buf generate",
    "clean": "rm -rf ../schema-gen/ts ../schema-gen/python ../schema-gen/rust",
    "build": "bun run lint && bun run generate"
  },
  "devDependencies": {
    "@bufbuild/protoc-gen-es": "2.10.2",
    "@connectrpc/protoc-gen-connect-es": "1.7.0"
  }
}
